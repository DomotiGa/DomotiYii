    "keywords": ["bootstrap", "yii"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christoffer Niska",
            "email": "christoffer.niska@gmail.com",
            "homepage": "http://www.cniska.net/"
        },
        {
            "name": "Antonio Ramirez",
            "email": "ramirez.cobos@gmail.com",
